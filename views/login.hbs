<h1>Login</h1>
<form id="registrarse" action="/api/sessions/login"  method="post">
    <table>
        <thead>
        </thead>
        <tbody>
            <tr class="border-0">
                <td><label for="email">Email:</label></td>
                <td><input type="text" id="email" name="email" ></td>
                <td><input type="checkbox" name="eliminaCookie" class="form-check-input" id="eliminaCookie" checked="checked" ></td>
            </tr>
            <tr class="border-0">
                <td><label for="password">Password:</label></td>
                <td><input type="password" id="password" name="password" ></td>
            </tr>
        </tbody>
    </table>
    <button type="submit" class="btn btn-success" id="sesion" value="Iniciar sesion">Iniciar sesion</button>
</form>
    <a href="/api/sessions/passwordRestore"> 
        <button type="submit" class="btn btn-success" id="restaurar" value="Restaurar">Restaurar</button>
    </a>
<hr>
<div>
    <a href="/github">
        <button class="btn btn-success" >Entrar con github</button>
    </a>
</div>

